{
  "name": "collect-es",
  "version": "2.0.0",
  "packageManager": "pnpm@7.4.0",
  "description": "typescript-template",
  "keywords": [
    "typescript",
    "template"
  ],
  "license": "MIT",
  "author": "peterroe",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "release": "np"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "eslint": "8.18.0",
    "esno": "0.16.3",
    "happy-dom": "^6.0.3",
    "np": "7.6.2",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "unplugin-auto-import": "^0.9.3",
    "vite": "2.9.14",
    "vite-plugin-dts": "^1.3.0",
    "vitest": "0.16.0"
  }
}